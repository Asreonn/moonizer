{
  "name": "moonizer",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "build:locales": "cd src/locales && node build-locales.js",
    "validate:locales": "cd src/locales && node validate-locales.js",
    "watch:locales": "cd src/locales && node build-locales.js --watch"
  },
  "keywords": [
    "typescript",
    "web",
    "vite",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "description": "Moonizer - Data Analysis and Visualization Tool",
  "dependencies": {
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "immer": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.0.1",
    "react-router-dom": "^6.8.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  }
}
